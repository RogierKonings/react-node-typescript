{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:server": "cross-env NODE_ENV=development ts-node-dev --inspect --ignore 'client/' ./application/server/index",
    "start:client": "cross-env NODE_ENV=development tsc && node ./application/client/devServer",
    "start:database": "cross-env mongod --config /usr/local/etc/mongod.conf",
    "start:mock": "ts-node-dev ./mock-server/server.js",
    "dev:server": "cross-env npm run start:server & npm run start:database",
    "dev:client": "cross-env npm run start:client & npm run start:mock",
    "start": "cross-env npm run start:client & npm run dev:server",
    "build:client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RogierKonings/react-node-typescript"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "typescript"
  ],
  "author": "Rogier Konings <rogier_konings@hotmail.com> (http://github.com/RogierKonings)",
